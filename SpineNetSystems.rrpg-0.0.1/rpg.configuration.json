{
	"comments": {
		// symbol used for single line comment. Remove this entry if your language does not support line comments
		"lineComment": [["//"]],
		// symbols used for start and end a block comment. Remove this entry if your language does not support block comments
		"blockComment": [["/*"],["*/"]]
	},
	// symbols used as brackets
	"brackets": [
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["if", "endif"],
		["do", "enddo"],
		["dow", "enddo"],
		["dou", "enddo"],
		["begsr", "endsr"],
		["dcl-proc", "end-proc"],
		["dcl-ds", "end-ds"],
		["dcl-pr", "end-pr"],
		["dcl-pi", "end-pi"],
		["select", "endsl"],
		["for", "endfor"],
		["/exec sql", "/end-exec"],
		["/free", "/end-free"]
	],
	   "autoClosingPairs": [
       { "open": "(", "close": ")" },
	   { "open": "\"", "close": "\"" },
	   { "open": "'", "close": "'" },
	   { "open": "/free", "close": "/end-free"}
   ]
}